let KidsGamesPage = class KidsGamesPage {
    constructor(speechRecognition) {
        this.speechRecognition = speechRecognition;
        this.bgcolor = 'white';
    }
    ngOnInit() {
        this.speechRecognition.hasPermission()
            .then((hasPermission) => {
            if (!hasPermission) {
                this.speechRecognition.requestPermission()
                    .then(() => console.log('Granted'), () => console.log('Denied'));
            }
        });
    }
    start() {
        this.speechRecognition.startListening()
            .subscribe((matches) => {
            this.bgcolor = matches[0];
        }, (onerror) => console.log('error:', onerror));
    }
};
KidsGamesPage.ctorParameters = () => [
    { type: _ionic_native_speech_recognition_ngx__WEBPACK_IMPORTED_MODULE_4__["SpeechRecognition"] }
];
